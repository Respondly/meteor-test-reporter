@import 'test-runner.import'


.tr-result-error
  position relative
  margin-bottom 8px
  padding 14px
  background $TR_PINK
  border-radius 3px
  > h2
    color white !important

  > .tr-trace
    color rgba(white, 0.6)
    font-size 12px
    line-height 14px
    margin-top 8px
    padding-top 8px
    border-top solid 1px rgba(white, 0.4)


