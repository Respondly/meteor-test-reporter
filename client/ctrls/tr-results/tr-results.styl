@import 'test-runner.import'


.tr-results
  font-family $TR_FONT_SANS
  padding-bottom 50px

  h1, h2, h3, h4
    padding 0
    margin 0
    font-weight 400
    font-size 16px
    color darken(white, 80%)

  > .tr-thumbs-up
    package-image 'respondly:test-reporter', 'thumbs-up.svg', 202, 219
    abs-center-x()
    top 120px
    opacity 0.1

